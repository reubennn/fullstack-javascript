{
  "name": "learning-fullstack-javascript-development",
  "version": "1.0.0",
  "description": "Learn by doing! Learn full-stack JavaScript development by building a web application with MongoDB, Node.js, and React.js.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,ejs,html --exec babel-node server/server.js --ignore public/",
    "dev-server": "nodemon -e js,ejs,html --exec babel-node server/server.js --ignore public/",
    "dev-client": "webpack-dev-server --open",
    "dev": "webpack -wd",
    "load-data": "npx babel-node --presets @babel/preset-env mongo/scripts/loadTestData.js",
    "update-data": "npx babel-node --presets @babel/preset-env mongo/scripts/updateTestData.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reubennn/fullstack-javascript.git"
  },
  "author": "Reuben Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reubennn/fullstack-javascript/issues"
  },
  "homepage": "https://github.com/reubennn/fullstack-javascript#readme",
  "dependencies": {
    "axios": "^1.6.3",
    "body-parser": "^1.19.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "mongodb": "^3.6.4",
    "node-sass-middleware": "^0.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.7.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.4.0",
    "nodemon": "^2.0.4",
    "react-test-renderer": "^16.13.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "^2.25.0"
  }
